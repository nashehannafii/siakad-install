sudo apt autoremove
sudo apt update
sudo apt upgrade

sudo apt install git
sudo apt update

sudo apt install php-cli unzip
curl -sS https://getcomposer.org/installer -o /tmp/composer-setup.php
HASH=`curl -sS https://composer.github.io/installer.sig`
echo $HASH
sudo php /tmp/composer-setup.php --install-dir=/usr/local/bin --filename=composer
sudo apt update

sudo composer self-update --1 -y

sudo apt update
sudo apt install npm -y
sudo apt update

cd /var/www/html/
sudo git clone https://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@github.com/virgantara/siakadv2.git

sudo git clone https://gitlab.com/virgantara/central.git

sudo mv siakadv2 siakad

cd /var/www/html/siakad/
composer install
npm install

cd /var/www/html/siakad/_protected/config/


cd /var/www/html/central/
composer install
npm install
